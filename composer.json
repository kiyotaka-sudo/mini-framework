{
  "name": "kiyotaka-sudo/mini-framework",
  "description": "Un mini-framework PHP pour apprendre",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\": "database/"
    }
  },
  "authors": [
    {
      "name": "Equipe Framework",
      "email": "team@example.com"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "^10.4"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable"
}
